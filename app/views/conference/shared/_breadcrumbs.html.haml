.box-shadow-medium.px-3.px-md-4.py-2.mb-4
  .container-md
    %nav{ aria: { label: 'Breadcrumb' } }
      %ol
        - ancestors.each do |ancestor|
          %li.breadcrumb-item
            %a= link_to ancestor.menu_title, ancestor.materialized_path, class: 'nav-link'
        %li.breadcrumb-item.breadcrumb-item-selected.text-gray= menu_title
