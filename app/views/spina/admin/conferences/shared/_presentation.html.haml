%tr{ data: { presentation_id: presentation.id } }
  %td= presentation.conference.name
  %td= link_to presentation.title, spina.edit_admin_conferences_presentation_path(presentation)
  %td
    %ul
      - presentation.presenters.each do |presenter|
        %li= link_to presenter.full_name, spina.edit_admin_conferences_delegate_path(presenter)
  %td.align-right
    = link_to spina.edit_admin_conferences_presentation_path(presentation), class: 'button button-small button-link' do
      = icon('pencil-outline')
      = t('spina.edit')
